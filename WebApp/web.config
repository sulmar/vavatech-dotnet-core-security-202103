<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.web>
		<httpRuntime enableVersionHeader="false"/>
	</system.web>
	<system.webServer>
		<security>
			<requestFiltering removeServerHeader="true" />
		</security>
		<httpProtocol>
			<customHeaders>
				<remove name="X-Powered-By"/>
				<add name="version" value="1.0" />				
			</customHeaders>
		</httpProtocol>
	</system.webServer>
</configuration>